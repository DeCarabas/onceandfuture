{
  "parser": "babel-eslint",
  "plugins": [ "react" ],
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "es6": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "spread": true,
    "templateStrings": true,
    "jsx": true
  },
  "rules": {
    "consistent-return": [0],
    "key-spacing": [0],
    "new-cap": [0],
    "no-global-assign": ["error"],
    "no-multi-spaces": [0],
    "no-shadow": [0],
    "no-undef": ["error"],
    "no-unused-vars": [1],
    "no-use-before-define": [2, "nofunc"],
    "quotes": [0],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  }
}
